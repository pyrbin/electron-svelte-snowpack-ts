{
  "name": "electron-svelte-snowpack-ts",
  "version": "0.0.1",
  "description": "electron-svelte-snowpack-ts",
  "private": true,
  "author": {
    "name": "Sample Sampleton",
    "email": "sample@sample.com"
  },
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dev": "concurrently npm:dev:run npm:dev:serve",
    "dev:run": "yarn start -- -- dev",
    "dev:serve": "snowpack dev",
    "build": "snowpack build"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "minimal_setup"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "svelte": "^3.25.1",
    "tailwindcss": "^1.8.10"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@snowpack/app-scripts-svelte": "^1.8.4",
    "@tsconfig/svelte": "^1.0.10",
    "@types/snowpack-env": "^2.3.0",
    "concurrently": "^5.3.0",
    "electron": "10.1.2",
    "prettier": "^2.1.2",
    "snowpack": "^2.11.1",
    "svelte-check": "^1.0.44",
    "svelte-preprocess": "^4.3.0",
    "typescript": "^4.0.2"
  }
}